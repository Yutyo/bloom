{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ],
    "rules": {
        "arrow-parens": 0,
        "comma-dangle": 0,
        "quote-props": 0,
        "no-use-before-define": 0,
        "operator-linebreak": "after",
        "react/prop-types": 0,
        "react/destructuring-assignment": 0,
        "react/jsx-filename-extension": 0,
        "react/no-unescaped-entities": 0
    }
}
